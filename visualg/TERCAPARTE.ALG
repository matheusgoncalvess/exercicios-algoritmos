Algoritmo "TercaParte"

Var
     N, dobro, TercaParte: real



Inicio
      Escreva("Digite um número: ")
      leia(N)
      
      dobro <- N*2

      Escreval("O dobro de ", N, " é ", dobro)
      TercaParte <- N / 3
      
      Escreva("A terça parte de ", N, " é ", TercaParte)
      

      

      

      
      
      

      


// Exercicio 05
Fimalgoritmo