Algoritmo "nota"

Var
    N1, N2, media: real



Inicio
      Escreva("Nota 1: ")
      leia(N1)
      Escreva("Nota 2: ")
      leia(N2)
      
      media <- (N1 + N2) / 2
      
      Escreva("A média entre", N1, " e ", N2, " é igual a", media)
      


// Exercicio 05
Fimalgoritmo