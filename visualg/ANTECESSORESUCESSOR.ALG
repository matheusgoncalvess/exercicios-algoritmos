Algoritmo "AntecessorESucessor"

Var
    N, antecessor, sucessor: inteiro



Inicio
      Escreva("Digite um número: ")
      leia(N)
      
      antecessor <- N-1
      sucessor <- N+1
      
      Escreval(" O antecessor de ", N, " é", antecessor)
      Escreval(" O sucessor de ", N, " é", sucessor)
      
Fimalgoritmo
