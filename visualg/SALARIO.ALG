Algoritmo "salario"

Var
   nome: caractere
   salario: real


Inicio
     Escreval("Nome do funcionário: ")
     leia(nome)
     Escreval("Salário: ")
     leia(salario)
     
     Escreva("O funcionário ", nome, " tem um salário de R$", salario, " em Junho. ")

     
     // Exercicio 03


Fimalgoritmo