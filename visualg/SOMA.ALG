Algoritmo "soma"

Var
   num1, num2: inteiro
   valor: inteiro


Inicio
    Escreva("Digite um valor: ")
    leia(num1)
    Escreva("Digite outro valor: ")
    leia(num2)
    
    valor <- num1 + num2
    
    Escreva(" A soma entre", num1, " e ", num2, " é igual a ", valor)

     
     // Exercicio 04


Fimalgoritmo